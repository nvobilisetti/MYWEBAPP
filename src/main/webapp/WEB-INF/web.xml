<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<filter>
		<filter-name>LoggingFilter</filter-name>
		<display-name>LoggingFilter</display-name>
		<description></description>
		<filter-class>com.nareen.filters.LoggingFilter</filter-class>
	</filter>
	<filter>
		<filter-name>AuthenticationFilter</filter-name>
		<display-name>AuthenticationFilter</display-name>
		<description></description>
		<filter-class>com.nareen.filters.AuthenticationFilter</filter-class>
	</filter>
	<filter>
		<filter-name>PerformanceLogFilter</filter-name>
		<display-name>PerformanceLogFilter</display-name>
		<description></description>
		<filter-class>com.nareen.filters.PerformanceLogFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>LoggingFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>AuthenticationFilter</filter-name>
		<url-pattern>/CarServlet</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>AuthenticationFilter</filter-name>
		<url-pattern>/display-cars.jsp</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>AuthenticationFilter</filter-name>
		<url-pattern>/add-car.jsp</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>PerformanceLogFilter</filter-name>
		<url-pattern>/PerformanceLogFilter</url-pattern>
	</filter-mapping>
	<listener>
		<listener-class>com.nareen.listners.MyListner</listener-class>
	</listener>
	<servlet>
		<servlet-name>HelloWorld</servlet-name>
		<display-name>HelloWorld</display-name>
		<description></description>
		<servlet-class>com.nareen.webser.HelloWorld</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>CarServlet</servlet-name>
		<display-name>CarServlet</display-name>
		<description></description>
		<servlet-class>com.nareen.webser.CarServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>CarDAO</servlet-name>
		<display-name>CarDAO</display-name>
		<description></description>
		<servlet-class>com.nareen.webser.CarDAO</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>RegistrationServlet</servlet-name>
		<display-name>RegistrationServlet</display-name>
		<description></description>
		<servlet-class>com.nareen.webser.RegistrationServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<display-name>LoginServlet</display-name>
		<description></description>
		<servlet-class>com.nareen.webser.LoginServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>LogoutServlet</servlet-name>
		<display-name>LogoutServlet</display-name>
		<description></description>
		<servlet-class>com.nareen.webser.LogoutServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>HelloWorld</servlet-name>
		<url-pattern>/HelloWorld</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>CarServlet</servlet-name>
		<url-pattern>/CarServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>CarDAO</servlet-name>
		<url-pattern>/CarDAO</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>RegistrationServlet</servlet-name>
		<url-pattern>/RegistrationServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/LoginServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LogoutServlet</servlet-name>
		<url-pattern>/LogoutServlet</url-pattern>
	</servlet-mapping>
</web-app>
